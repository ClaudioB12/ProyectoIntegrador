  <body>
    <header>
        <div class="container-hero">
            <div class="container hero">
                <div class="customer-support">
                    <i class="fa-solid fa-headset"></i>
                    <div class="content-customer-support">
                        <span class="text">Soporte al cliente</span>
                        <span class="number">123-456-7890</span>
                    </div>
                </div>

                <div class="container-logo">
                    <i class="fa-solid fa-chair"></i>
                    <h1 class="logo"><a href="/">Ebanista</a></h1>
                </div>
                <!-- Bton para reemplazar -->
                <div class="container-user">
                  <div x-data="{ open: false }" class="relative ml-3">
                  <div>
                      <button @click="open = ! open" type="button" class="relative flex rounded-full bg-gray-800 text-lg focus:outline-none focus:ring-2 focus:ring-white focus:ring-offset-2 focus:ring-offset-gray-800" id="user-menu-button" aria-expanded="false" aria-haspopup="true">
                          <span class="absolute -inset-2"></span>
                          <span class="sr-only">Open user menu</span>
                          <i class="fa-solid fa-user"></i>
                      </button>
                  </div>

                    <!-- Dropdown menu del perfil, show/hide based on menu state -->
                    <div x-show="open" @click.outside="open = false" class="absolute right-0 z-10 mt-2 w-64 origin-top-right rounded-md bg-white py-1 shadow-lg ring-1 ring-black ring-opacity-5 focus:outline-none" role="menu" aria-orientation="vertical" aria-labelledby="user-menu-button" tabindex="-1">
                        <!-- Active: "bg-gray-100", Not Active: "" -->
                        <a href="/login" class="block px-6 py-3 text-lg text-gray-700" role="menuitem" tabindex="-1" id="user-menu-item-0">Login</a>
                    </div>
                </div>
                    <i class="fa-solid fa-basket-shopping"></i>
                    <!-- Fin del boton para reemplazar -->
                    
                    <div class="content-shopping-cart">
                        <span class="text">Carrito</span>
                        <span class="number">(0)</span>
                    </div>
                </div>
                

                <!-- Perfil dropdown del encabezado original -->
                
            </div>
        </div>

        <div class="container-navbar">
            <nav class="navbar container">
                <i class="fa-solid fa-bars"></i>
                <ul class="menu">
                    <li><a href="#">Inicio</a></li>
                    <li><a href="#">Sofas</a></li>
                    <li><a href="#">Mesas</a></li>
                    <li><a href="#">Comedores</a></li>
                    <li><a href="#">Más</a></li>
                    <li><a href="#">Blog</a></li>
                </ul>

                <form class="search-form">
                    <input type="search" placeholder="Buscar..." />
                    <button class="btn-search">
                        <i class="fa-solid fa-magnifying-glass"></i>
                    </button>
                </form>
            </nav>
        </div>
    </header>
